{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ESNext",
		"allowJs": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"rootDir": "src",
		"outDir": "dist",
		"strict": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"forceConsistentCasingInFileNames": true,
		"removeComments": true,
		"moduleResolution": "Node",
		"alwaysStrict": true
	},
	"exclude": [
		"node_modules",
		"./dist",
		"webpack.config.js"
	]
}